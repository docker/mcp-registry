{
  "name": "mixpeek-mcp",
  "displayName": "Mixpeek MCP Server",
  "description": "OpenAPI-driven MCP server for Mixpeek (stdio), with Docker image and npm launcher.",
  "source": {
    "type": "git",
    "url": "https://github.com/mixpeek/mcp-server"
  },
  "categories": ["retrieval", "multimodal", "data"],
  "license": "MIT",
  "homepage": "https://docs.mixpeek.com/overview/introduction",
  "maintainers": [
    { "name": "Mixpeek", "url": "https://mixpeek.com" }
  ],
  "docker": {
    "image": "mixpeek/mcp:0.1.0",
    "command": ["python", "server.py"],
    "env": [
      "MIXPEEK_API_KEY",
      "MIXPEEK_NAMESPACE",
      "MIXPEEK_API_BASE",
      "MIXPEEK_OPENAPI_URL"
    ],
    "ports": []
  },
  "npm": {
    "package": "@mixpeek/mcp",
    "command": "mixpeek-mcp"
  },
  "pypi": {
    "package": "mixpeek-mcp",
    "entrypoint": "mixpeek-mcp"
  },
  "mcp": {
    "transport": "stdio",
    "tools": "auto-from-openapi"
  }
}


